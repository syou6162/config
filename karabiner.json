                                    {
                                        "type": "frontmost_application_if",
                                        "bundle_identifiers": [
                                            "^com\\.googlecode\\.iterm2$",
                                            "^com\\.apple\\.Terminal$"
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "description": "Map Ctrl-b to Left Arrow (backward-char)",
                        "manipulators": [
                            {
                                "type": "basic",
                                "from": {
                                    "key_code": "b",
                                    "modifiers": {
                                        "mandatory": ["control"],
